.screen#main
  .header-container
    .header.artists
      .fa.fa-user
      | All Artists
    .header.lottery
      .fa.fa-check
      | Lottery
    .header.standby
      .fa.fa-clock-o
      | Standby
    .header.seated
      .fa.fa-sign-in
      | Seated
  div(style='height:calc(100% - 30px)')
    .col-xs-3
      .content
        .artists.height-full
          - var n = 0
          while n < 100
            a.line(href='#')
              | test
              = n++
    .col-xs-3
      .content
        .lottery.height-full
          - var n = 0
          while n < 100
            a.line(href='#')
              | test
              = n++
    .col-xs-3
      .content
        .standby.height-full
          - var n = 0
          while n < 100
            a.line(href='#')
              | test
              = n++
    .col-xs-3
      .content
        .seated.height-full
          - var n = 0
          while n < 100
            a.line(href='#')
              | test
              = n++
